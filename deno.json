{
    "tasks": {
        "dev": "deno run -A cmd/deno-hono/main.ts --watch",
        "cache": "deno cache -r deps.ts"
    },
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx"
    },
    "imports": {
        "$std/": "https://deno.land/std@0.202.0/",
        "$ulid/": "https://deno.land/x/ulid@v0.3.0/",
        "$valibot/": "https://deno.land/x/valibot@v0.18.0/",
        "$hono/": "https://deno.land/x/hono@v3.7.3/",
        "hono/jsx/jsx-runtime": "https://deno.land/x/hono@v3.7.3/jsx/jsx-runtime.ts",
        "~/deps.ts": "./deps.ts"
    }
}